<?xml version="1.0" encoding="UTF-8"?>
<plugin id="cordova-plugin-ios-app-capabilities" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0">
  <name>App Capabilities</name>
  <platform name="ios">
    <config-file target="*-Debug.plist" parent="com.apple.developer.associated-domains">
      <array>
        <string>applinks:www.wg-gesucht.de</string>
      </array>
    </config-file>
    <config-file target="*-Release.plist" parent="com.apple.developer.associated-domains">
      <array>
        <string>applinks:www.wg-gesucht.de</string>
      </array>
    </config-file>
    		<config-file parent="com.apple.developer.associated-domains" target="*/Entitlements-Debug.plist">
			 <array>
				<string>applinks:www.wg-gesucht.de</string>
			  </array>
		</config-file>
  </platform>
</plugin>
